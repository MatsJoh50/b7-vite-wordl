{{! 
    const hsSchema = new mongoose.Schema({
    name: String,
    startTime: Date,
    dupe: Boolean,
    value: Number,
    endTime: Date,
    guesses: Number,
}) }}

<h1 class="text-3xl">The Honorble Few</h1>
<div
  class="m-0 h-full max-w-full w-full flex flex-col "
>
  {{#each output}}
  <div class="border m-3 mx-auto flex flex-col w-[60vw] p-3">
    <div class="flex flex-row justify-evenly mb-3">
      <p>Name: {{this.name}}</p>
      <p>Duplicates: {{this.dupe}}</p>
      <p>Length of word: {{this.value}}</p>
    </div>

    <div class="flex flex-row justify-evenly">
      <p>Time: {{this.startTime}}</p>
      <p>Score:</p>
    </div>
  </div>
  {{/each}}
</div>
  